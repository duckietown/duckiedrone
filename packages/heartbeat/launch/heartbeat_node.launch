<launch>
  <arg name="veh" doc="Name of vehicle. ex megaman"/>
  <arg name="robot_type" default="duckiedrone"  
		doc="The type of the robot: should be duckiebot, 
		watchtower, duckiedrone or traffic_light"/>

  <group ns="$(arg veh)" >

  <node Heartbeat node pkg="pidrone_pkg" 
        type="heartbeat_publisher_NEVER_RUN_ON_DRONE.py" 
        name="heartbeat_publisher_NEVER_RUN_ON_DRONE" 
        output="screen"/>

  </group>
</launch>
