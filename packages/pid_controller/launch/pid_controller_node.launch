<launch>
  <arg name="veh"/>
  <arg name="pkg_name" value="pid_controller"/>
  <arg name="node_name" default="pid_controller_node"/>

  <group ns="$(arg veh)">

    <node pkg="$(arg pkg_name)" type="$(arg node_name).py" name="$(arg node_name)" output="screen">
    </node>
    
  </group>

</launch>

